pipeline {
    agent any

    stages {
        stage("clean space") {
             steps {
                timeout(time: 5, unit: 'MINUTES') {
                bat "rmdir /s /q ."
        }
    }
        }
        stage('pull the project') {
            steps {
                bat "git clone https://github.com/azeez-abp/node_project.git"
                bat "cd node_project"
            }
        }
        
        stage('install dependencies') {
            steps {
                bat "npm install"
            }
        }
        
        stage('build front end') {
            steps {
                bat "npm run build"  // Corrected the npm build command
            }
        }
        
        stage('run tests with mocha and chai') {
            steps {
                bat "npm install pm2@latest -g"
            }
        }
        
       
        stage('deploy') {
            steps {
                bat "pm2 startOrRestart server.js"  // Corrected the command
            }
        }
    }
}

